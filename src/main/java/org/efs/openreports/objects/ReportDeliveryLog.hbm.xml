<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">

	<class name="org.efs.openreports.objects.ReportDeliveryLog" table="REPORT_DELIVERY_LOG">
		
		<id name="id" column="DELIVERY_LOG_ID" type="java.lang.Integer">            
			<generator class="native"/>            
        </id>
        
        <property name="startTime" column="START_TIME"/>
        
        <property name="endTime" column="END_TIME"/>
        
        <property name="status" column="STATUS"/>
        
        <property name="message" column="MESSAGE" type="text"/>
        
        <property name="deliveryMethod" column="DELIVERY_METHOD"/>   
       
    </class>
    
</hibernate-mapping>
